<UserControl x:Class="OnewheroVisitorManagement.View.AdminEvents"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OnewheroVisitorManagement.View"
             xmlns:vm="clr-namespace:OnewheroVisitorManagement.ViewModel"
             mc:Ignorable="d" >

    <UserControl.DataContext>
        <vm:AdminEventsVM/>
    </UserControl.DataContext>
    
    <Grid>
        <Border CornerRadius="0,20,20,0" Background="White">
            <TextBlock Text="Admin Events Page"
               Style="{StaticResource TitleText_Style}"/>
        </Border>
    </Grid>
</UserControl>
